{
  "compilerOptions": {
    "outDir": "dist",
    "target": "es6",
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": ".",
    "paths": {
      "config": [
        "./config"
      ],
      "controllers": [
        "./api/controllers"
      ],
      "middleware": [
        "./middleware"
      ],
      "models": [
        "./api/models"
      ],
      "routes": [
        "./api/routes"
      ]
    }
  },
}